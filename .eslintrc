{
  "parser": "@babel/eslint-parser",
  "extends": ["eslint:recommended"],
  "env": {
    "es6": true,
    "node": true
  },
  "globals": {},
  "parserOptions": {
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "classes": true
    },
    "sourceType": "module"
  },
  "rules": {
    "strict": 2,
    "no-console": 0,
    "no-unused-vars": [
      "error",
      {
        "vars": "all",
        "args": "none",
        "ignoreRestSiblings": false
      }
    ]
  }
}
